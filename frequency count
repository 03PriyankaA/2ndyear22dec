#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int i,len,count1=0,count2=0;
    char str[1000];
    gets(str);
    len=strlen(str);
    for(i=0;i<=len;i++){
        if(str[i]>='A' && str[i]<='Z'){
            count1++;
        }
         if(str[i]>='a' && str[i]<='z'){
            count2++;
        }
    }
    printf("%d %d",count1,count2);
    return 0;
}
